{
	"target": "esnext",
	"module": "esnext",
	"moduleResolution": "node",
	"allowSyntheticDefaultImports": true,
	"lib": [
		"esnext",
		"esnext.string",
		"dom",
		"dom.iterable",
		"es5"
	],
	"esModuleInterop": true,
	"removeComments": true,
	"experimentalDecorators": true,
	"resolveJsonModule": true,
	"jsx": "react-jsx",
	"jsxImportSource": "preact",
	"noEmit": true,
	"allowJs": true
}
