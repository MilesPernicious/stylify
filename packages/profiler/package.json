{
	"name": "@stylify/profiler",
	"description": "Profiler for @stylify/packages",
	"version": "0.0.1",
	"main": "dist/index.js",
	"module": "dist/index.module.js",
	"unpkg": "dist/profiler.min.js",
	"jsdelivr": "dist/profiler.min.js",
	"types": "types",
	"scripts": {
		"init": "yarn install",
		"watch": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js -w src",
		"build": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js && yarn types",
		"types": "yarn tsc ./src/index.ts --d --moduleResolution node --emitDeclarationOnly --target esnext --jsx react-jsx --jsxImportSource preact --outDir types"
	},
	"files": [
		"dist",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/profiler"
	},
	"keywords": [],
	"author": "Vladimír Macháček",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"homepage": "https://stylify.dev",
	"devDependencies": {
		"@babel/core": "^7.12",
		"@babel/plugin-proposal-class-properties": "^7.14",
		"@babel/plugin-proposal-object-rest-spread": "^7.14",
		"@babel/preset-env": "^7.12",
		"@babel/preset-react": "^7.12",
		"@babel/preset-typescript": "^7.12",
		"@rollup/plugin-babel": "^5.2",
		"@rollup/plugin-commonjs": "^17.0",
		"@rollup/plugin-node-resolve": "^11.0",
		"@rollup/plugin-replace": "^2.3",
		"@stylify/stylify": "^0.0",
		"@types/node": "^14.14",
		"htm": "^3.0",
		"identity-obj-proxy": "^3.0.0",
		"postcss": "^8.2",
		"postcss-url": "^10.1",
		"preact": "^10.5",
		"promise": "^8.1",
		"rollup": "^2.35",
		"rollup-plugin-banner": "^0.2",
		"rollup-plugin-postcss": "^4.0",
		"rollup-plugin-terser": "^7.0",
		"rollup-plugin-typescript2": "^0.30",
		"tslib": "^2.0",
		"typescript": "^4.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
