{
	"target": "es6",
	"moduleResolution": "node",
	"allowSyntheticDefaultImports": true,
	"lib": [
		"es5",
		"esnext",
		"es6",
		"dom"
	],
	"esModuleInterop": true,
	"removeComments": true,
	"experimentalDecorators": true,
	"resolveJsonModule": true,
	"noEmit": true,
	"jsx": "react-jsx",
	"jsxImportSource": "preact",
	"allowJs": true
}
