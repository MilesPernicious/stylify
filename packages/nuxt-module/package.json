{
	"name": "@stylify/nuxt-module",
	"description": "Nuxt module for @stylify/stylify.",
	"version": "0.0.1",
	"main": "lib/index.js",
	"module": "esm/index.js",
	"types": "types",
	"scripts": {
		"init": "yarn install",
		"watch": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js -w ./src",
		"build": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js && yarn types",
		"types": "yarn tsc ./src/index.ts --d --moduleResolution node --emitDeclarationOnly --allowSyntheticDefaultImports --target esnext --jsx react-jsx --jsxImportSource preact --outDir types",
		"plg:build": "cd playground && yarn install && yarn build",
		"plg:dev": "cd playground && yarn install && yarn dev",
		"plg:start": "cd playground && yarn install && yarn build && yarn start"
	},
	"files": [
		"esm",
		"lib",
		"types",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/nuxt-module"
	},
	"keywords": [],
	"author": "Vladimír Macháček",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"homepage": "https://stylify.dev",
	"devDependencies": {
		"autoprefixer": "^10.3",
		"@babel/core": "^7.12",
		"@babel/plugin-proposal-class-properties": "^7.14",
		"@babel/plugin-proposal-object-rest-spread": "^7.14",
		"@babel/preset-env": "^7.12",
		"@babel/preset-react": "^7.12",
		"@babel/preset-typescript": "^7.12",
		"@rollup/plugin-babel": "^5.2",
		"@rollup/plugin-commonjs": "^17.0",
		"@rollup/plugin-node-resolve": "^11.0",
		"@rollup/plugin-replace": "^2.3",
		"rollup-plugin-typescript2": "^0.30",
		"@types/node": "^14.14",
		"postcss-js": "^3.0",
		"promise": "^8.1",
		"rollup": "^2.35",
		"rollup-plugin-banner": "^0.2",
		"rollup-plugin-terser": "^7.0",
		"tslib": "^2.0",
		"typescript": "^4.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@stylify/autoprefixer": "^0.0",
		"@stylify/stylify": "^0.0",
		"@stylify/profiler": "^0.0",
		"loader-utils": "^2.0"
	}
}
