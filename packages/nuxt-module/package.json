{
	"name": "@stylify/nuxt-module",
	"description": "Nuxt module for @stylify/stylify.",
	"version": "0.0.1",
	"main": "./lib/index.js",
	"module": "./esm/index.js",
	"types": "./types",
	"scripts": {
		"watch": "yarn build -w src",
		"build": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js && yarn types",
		"types": "yarn tsc ./src/index.ts --d --moduleResolution node --emitDeclarationOnly --allowSyntheticDefaultImports --target esnext --jsx react-jsx --jsxImportSource preact --outDir types",
		"lint:check": "eslint ./src/*.ts",
		"lint:fix": "yarn lint:check --fix",
		"plg:build": "cd playground && yarn install && yarn build",
		"plg:dev": "cd playground && yarn install && yarn dev",
		"plg:start": "cd playground && yarn install && yarn build && yarn start"
	},
	"files": [
		"esm",
		"lib/",
		"types/",
		"LICENSE",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/nuxt-module"
	},
	"keywords": [],
	"author": "Vladimír Macháček",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"homepage": "https://github.com/stylify/packages/#readme",
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@babel/preset-typescript": "^7.12.7",
		"@rollup/plugin-babel": "^5.2",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-replace": "^2.3.4",
		"@rollup/plugin-typescript": "^8.1",
		"@stylify/stylify": "^0.0.1",
		"@types/node": "^14.14",
		"@typescript-eslint/eslint-plugin": "^4.11",
		"@typescript-eslint/parser": "^4.11",
		"eslint": "^7.16",
		"promise": "^8.1",
		"rollup": "^2.35",
		"rollup-plugin-banner": "^0.2",
		"rollup-plugin-terser": "^7.0",
		"tslib": "^2.0",
		"typescript": "^4.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"loader-utils": "^2.0.0"
	}
}
