{
	"name": "@stylify/autoprefixer",
	"description": "Autoprefixer browser/ssr integration for @stylify/stylify.",
	"version": "0.0.1",
	"main": "./lib/index.js",
	"module": "./esm/index.js",
	"types": "./types",
	"scripts": {
		"init": "yarn install",
		"watch": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js -w src",
		"build": "node --max-old-space-size=8192 node_modules/.bin/rollup -c rollup.config.js && yarn types",
		"types": "yarn tsc ./src/index.ts --d --moduleResolution node --emitDeclarationOnly --allowSyntheticDefaultImports --target esnext --jsx react-jsx --jsxImportSource preact --outDir types",
		"lint:check": "eslint ./src/*.ts",
		"lint:fix": "yarn lint:check --fix",
		"jest:test": "node --experimental-modules node_modules/.bin/jest"
	},
	"files": [
		"esm/",
		"lib/",
		"types/",
		"LICENSE",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git",
		"directory": "packages/autoprefixer"
	},
	"keywords": [],
	"author": "Vladimír Macháček",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"homepage": "https://github.com/stylify/packages/#readme",
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.14.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"@babel/preset-env": "^7.15.0",
		"@rollup/plugin-babel": "^5.2",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-replace": "^2.3.4",
		"@rollup/plugin-typescript": "^8.1",
		"@stylify/stylify": "0.0.1",
		"@types/jest": "^26.0.24",
		"@types/node": "^14.14",
		"@typescript-eslint/eslint-plugin": "^4.11",
		"@typescript-eslint/parser": "^4.11",
		"babel-jest": "^26.6.3",
		"eslint": "^7.16",
		"jest": "^27.0",
		"jest-environment-jsdom": "^27.0",
		"jest-environment-node": "^27.0",
		"promise": "^8.1",
		"rollup": "^2.35",
		"rollup-plugin-banner": "^0.2",
		"rollup-plugin-terser": "^7.0",
		"ts-jest": "^27.0",
		"tslib": "^2.0",
		"typescript": "^4.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"autoprefixer": "^10.3.1",
		"postcss-js": "^3.0.3"
	},
	"browserslist": [
		"> 0.25%"
	]
}
