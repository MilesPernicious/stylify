{
	"name": "@stylify/packages",
	"version": "0.0.1",
	"description": "Stylify monorepo",
	"author": "Vladimír Macháček",
	"license": "MIT",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"repo:init": "yarn install && yarn link && yarn bootstrap",
		"bootstrap": "lerna bootstrap --force-local",
		"watch": "lerna run --stream --parallel --concurrency=2 watch ",
		"build": "lerna run --stream --parallel --concurrency=2 build",
		"build:all": "yarn stylify:build && yarn autoprefixer:build && yarn nuxtmodule:build",
		"types": "lerna run --parallel --concurrency=2 types",
		"autoprefixer:init": "lerna run init --scope=@stylify/autoprefixer",
		"autoprefixer:build": "yarn build --scope=@stylify/autoprefixer",
		"autoprefixer:watch": "yarn watch --scope=@stylify/autoprefixer",
		"autoprefixer:types": "yarn types --scope=@stylify/autoprefixer",
		"autoprefixer:lint:check": "yarn lint:check --scope=@stylify/autoprefixer",
		"autoprefixer:lint:fix": "yarn lint:fix --scope=@stylify/autoprefixer",
		"autoprefixer:test": "yarn jest:test --scope=@stylify/autoprefixer",
		"stylify:init": "lerna run init --scope=@stylify/stylify",
		"stylify:gnp": "lerna run gnp --scope=@stylify/stylify",
		"stylify:build": "yarn build --scope=@stylify/stylify",
		"stylify:watch": "yarn watch --scope=@stylify/stylify",
		"stylify:types": "yarn types --scope=@stylify/stylify",
		"stylify:lint:check": "yarn lint:check --scope=@stylify/stylify",
		"stylify:lint:fix": "yarn lint:fix --scope=@stylify/stylify",
		"stylify:test": "yarn jest:test --scope=@stylify/stylify",
		"nuxtmodule:init": "lerna run init --scope=@stylify/nuxt-module",
		"nuxtmodule:build": "yarn build --scope=@stylify/nuxt-module",
		"nuxtmodule:watch": "yarn watch --scope=@stylify/nuxt-module",
		"nuxtmodule:lint:check": "yarn lint:check --scope=@stylify/nuxt-module",
		"nuxtmodule:lint:fix": "yarn lint:fix --scope=@stylify/nuxt-module",
		"nuxtmodule:plg:build": "lerna run --stream plg:build --scope=@stylify/nuxt-module",
		"nuxtmodule:plg:dev": "lerna run --stream plg:dev --scope=@stylify/nuxt-module",
		"nuxtmodule:plg:start": "lerna run --stream plg:start --scope=@stylify/nuxt-module",
		"nuxtmodule:test": "yarn jest:test --scope=@stylify/nuxt-module",
		"lint:check": "lerna run lint:check",
		"lint:fix": "lerna run lint:fix",
		"jest:test": "lerna run jest:test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stylify/packages.git"
	},
	"keywords": [

	],
	"bugs": {
		"url": "https://github.com/stylify/packages/issues"
	},
	"homepage": "https://github.com/stylify/packages/#readme",
	"devDependencies": {
		"lerna": "^4.0"
	}
}
